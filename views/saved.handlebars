<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Title</th>
          <th scope="col">Link</th>
          <th scope="col">Content</th>
        </tr>
      </thead>
      <tbody id="savedItems">
        {{#each result}}
        <tr>
          <td>{{title}}</td>
          <td>{{link}}</td>
          <td>{{content}}</td>
          <td><button type="button" class="btn btn-danger" id="del-btn" data-id={{_id}}>Delete</button></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  <div class="col-md-2"></div>
</div>